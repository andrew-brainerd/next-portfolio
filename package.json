{
  "name": "next-portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "build": "next build",
    "clean": "rm -rf node_modules .next",
    "dev": "next dev",
    "lint": "pnpm lint:js && pnpm lint:css",
    "lint:fix": "pnpm lint:js:fix && pnpm lint:css:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "prepare": "husky",
    "start": "next start",
    "update-packages": "npx npm-check-updates@latest -u && pnpm i && git add . && git commit -m \"Update packages\"",
    "verify": "npx tsc && pnpm lint"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.962.0",
    "@aws-sdk/s3-request-presigner": "^3.962.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@lottiefiles/react-lottie-player": "^3.6.0",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/leaflet": "^1.9.21",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "firebase": "^12.7.0",
    "firebase-admin": "^13.6.0",
    "howlongtobeat": "^1.8.0",
    "leaflet": "^1.9.4",
    "next": "16.1.1",
    "postcss": "^8.5.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-leaflet": "^5.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "6.0.0-dev.20251017",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@typescript-eslint/parser": "^8.52.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss-html": "^1.8.0",
    "sass": "^1.97.2",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-order": "^7.0.1"
  },
  "lint-staged": {
    "**/*": "npx prettier --write --ignore-unknown"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.8",
      "@types/react-dom": "19.2.3"
    },
    "onlyBuiltDependencies": [
      "@firebase/util",
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "protobufjs",
      "sharp",
      "unrs-resolver"
    ]
  }
}
